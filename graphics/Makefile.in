# The following is all boiler plate 
#------------------------------------------------------------------------------
# Autoconfig defines that can be overridden in user.make

@MAKEINCLUDE@ @MAKEQUOTE@../system.make@MAKEQUOTE@

#------------------------------------------------------------------------------
# Include user-editable defines.

@MAKEINCLUDE@ @MAKEQUOTE@../user.make@MAKEQUOTE@

#------------------------------------------------------------------------------
# Include the generated targets

@MAKEINCLUDE@ @MAKEQUOTE@targets.make@MAKEQUOTE@


VTK_LIB_FILE  = libVTK${ME}${VTK_LIB_EXT}
TCL_LIB_FILE  = libVTK${ME}Tcl${VTK_LIB_EXT}

# add in any extra libraies here
KIT_LIBS = @TKWIDGET@

#JAVA_O_ADD = vtkJavaPanel.o 
JAVA_CODE_ADD = ../java/vtk/vtkPanel.class

java/vtk_vtkPanel.h: ../java/vtk/vtkPanel.java
	rm -f java/vtk_vtkPanel.h; ${JAVAH} -d java vtk.vtkPanel

java/vtk_vtkPanel.c: ../java/vtk/vtkPanel.java
	rm -f java/vtk_vtkPanel.c; ${JAVAH} -stubs -d java vtk.vtkPanel

vtkJavaPanel.o: java/vtk_vtkPanel.h java/vtk_vtkPanel.c

# standard stuff here
KIT_OBJ = vtkXRenderWindowInteractor.o ${SRC_OBJ} 

KIT_TCL_OBJ = vtkXRenderWindowTclInteractor.o ${TCL_OBJ}

KIT_NEWS = ${TCL_NEWS} 

KIT_FLAGS = 
PYTHON_LIBS = @USE_GRAPHICS_PATENTED_LIBS@ -L../common -lVTKCommonPython ${XLIBS} ${GRAPHICS_API_LIBS}

