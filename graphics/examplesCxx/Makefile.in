#
# examples/cc//Makefile.in  --
#
# Makefile for Visualization Toolkit cc examples.
# 
#------------------------------------------------------------------------------
#
SHELL = /bin/sh
.SUFFIXES: .cc

#------------------------------------------------------------------------------
# Autoconfig defines that can be overridden in Config.mk

CC            = @CC@
CFLAGS        = @CFLAGS@
CXX           = @CXX@
CXXFLAGS      = @CXXFLAGS@
XCFLAGS       = @X_CFLAGS@
XLIBS         = @X_LIBS@
RANLIB        = @RANLIB@
srcdir        = @srcdir@
VPATH         = @srcdir@
prefix        = @prefix@
exec_prefix   = @exec_prefix@
XGL_HOME      = @XGL_HOME@
X_EXTRA_LIBS  = @X_EXTRA_LIBS@
X_PRE_LIBS    = @X_PRE_LIBS@
VTK_LIB_FILE  = @VTK_LIB_FILE@
DL_LIBS       = @DL_LIBS@
VTK_TEMPLATE_FLAGS = @VTK_TEMPLATE_FLAGS@
VTK_INLINE_FLAGS = @VTK_INLINE_FLAGS@

#------------------------------------------------------------------------------
# Include user-editable defines.

@MAKEINCLUDE@ @MAKEQUOTE@../../Config.mk@MAKEQUOTE@

#-----------------------------------------------------------------------------

EXAMPLES = VisQuad 
  
@sbr_obj@

@glr_obj@

@oglr_obj@

@xglr_obj@

#------------------------------------------------------------------------------

CXX_FLAGS = ${CPPFLAGS} ${XCFLAGS} ${CXXFLAGS} -I${srcdir}/../../common \
	${VTK_INLINE_FLAGS} ${VTK_TEMPLATE_FLAGS} \
	${XGLR_FLAGS} ${OGLR_FLAGS} ${SBR_FLAGS} ${GLR_FLAGS} \
	-D_HP_NO_FAST_MACROS

VTKLIB = ${VTK_LIB_FILE}

LDLIBS = -L../../common -lVTKCommon -L.. -lVTKGraphics ${XLIBS} ${OGLR_LIBS} ${GLR_LIBS} ${SBR_LIBS} ${XGLR_LIBS} ${DL_LIBS}

.cxx.o:
	${CXX} ${CXX_FLAGS} -c $< -o $@

#------------------------------------------------------------------------------
#

