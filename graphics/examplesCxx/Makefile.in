	quadricCut \
	singleSplat \
	tcutSph \
	vol
  
CXX_FLAGS = ${CPPFLAGS} ${CXXFLAGS} -I${srcdir}/../../common \
	${GRAPHICS_API_FLAGS} -D_HP_NO_FAST_MACROS

VTKLIB = ${VTK_LIB_FILE}

LDLIBS = ${VTK_LIB_FILE} ${XLIBS} ${GRAPHICS_API_LIBS} ${DL_LIBS} ${THREAD_LIBS}

.cxx.o:
	${CXX} ${CXX_FLAGS} -c $< -o $@

