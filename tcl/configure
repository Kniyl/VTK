        echo $ac_n "checking whether ${CXX} accepts +d""... $ac_c" 1>&6
        echo 'void f(){}' > conftest.cc
        if test -z "`${CXX} +d -c conftest.cc 2>&1`"; then
          echo "$ac_t""yes" 1>&6
